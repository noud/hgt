{% for type, messages in app.session.flashbag.all() %}
    {% for message in messages %}
        {% if type == 'error' %}
            <div class="alert alert-danger">
                {{ message }}
            </div>
        {% endif %}
        {% if type == 'success' %}
            <div class="alert alert-success">
                {{ message }}
            </div>
        {% endif %}
    {% endfor %}
{% endfor %}
